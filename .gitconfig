[credential]
	helper = store
[alias]
	s = status -u
	sw = switch
	cb = switch -c
	rs = restore
	b = branch
  l = log --pretty=format:"%C(yellow)%h\\ %Creset%s%C(red)%d%C(blue)\\ [%cn]\\ %cr"
	lg = log --graph --abbrev-commit --decorate --pretty=format:"%C(yellow)%h\\ %Creset%s%C(red)%d%C(blue)\\ [%cn]\\ %cr" --all
	c = commit -m
	ca = commit --amend --no-edit
	rb = rebase -i
	fa = fetch --all --prune --prune-tags
	mc = merge --no-ff
	mf = merge --ff-only
	st = stash
	stl = stash list
	pop = stash pop
[core]
	excludesfile = ~/.gitignore_global
