[credential]
	helper = store
[alias]
	s = status -u
	sw = switch
	cb = switch -c
	rs = restore
	b = branch
	ba = branch -avv
	l = log --pretty=format:"%C(yellow)%h\\ %Creset%s%C(red)%d%C(blue)\\ [%cn]\\ %cr"
	lg = log --graph --abbrev-commit --decorate --pretty=format:"%C(yellow)%h\\ %Creset%s%C(red)%d%C(blue)\\ [%cn]\\ %cr"
	la = log --pretty=format:"%C(yellow)%h\\ %Creset%s%C(red)%d%C(blue)\\ [%cn]\\ %cr" --all
	lga = log --graph --abbrev-commit --decorate --pretty=format:"%C(yellow)%h\\ %Creset%s%C(red)%d%C(blue)\\ [%cn]\\ %cr" --all
	c = commit -m
	ca = commit --amend --no-edit
	rb = rebase -i
	fa = fetch --all
	fap = fetch --all --prune --prune-tags
	mc = merge --no-ff
	mf = merge --ff-only
	st = stash
	stl = stash list
	pop = stash pop
[core]
	excludesfile = ~/.gitignore_global
[pull]
	rebase = true
[rebase]
	autoStash = true
[tag]
	sort = version:refname

